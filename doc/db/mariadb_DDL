
CREATE TABLE `user` (
   `ID` int(12) NOT NULL AUTO_INCREMENT,
   `USER_ID` varchar(100) NOT NULL COMMENT '아이디',
   `USER_NAME` varchar(100) DEFAULT NULL COMMENT '이름',
   `PHONE` varchar(30) DEFAULT NULL COMMENT '핸드폰',
   `REG_DATE` datetime NOT NULL DEFAULT current_timestamp() COMMENT '등록일',
   PRIMARY KEY (`ID`),
   KEY `USER_IDX1` (`USER_ID`)
 ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;